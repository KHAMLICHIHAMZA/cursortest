{
  "name": "mobile-agent",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "postinstall": "echo Skipping patch-expo-cli",
    "start": "expo start --tunnel",
    "dev": "expo start --tunnel",
    "tunnel": "expo start --tunnel",
    "lan": "expo start --lan",
    "android": "expo start --android --lan",
    "ios": "expo start --ios --lan",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:all": "node scripts/run-all-tests.js",
    "test:validation": "jest src/services/__tests__/loginSchema.test.ts src/utils/__tests__/validation.test.ts src/types/__tests__/types.test.ts",
    "test:integration": "jest src/services/__tests__/integration.test.ts --testTimeout=30000",
    "test:e2e": "echo 'Install Maestro first: see INSTALLATION_MAESTRO.md' && maestro test .maestro",
    "test:e2e:login": "maestro test .maestro/login.yaml",
    "test:e2e:bookings": "maestro test .maestro/bookings-flow.yaml",
    "test:e2e:checkin": "maestro test .maestro/checkin-flow.yaml"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@tanstack/react-query": "^5.17.0",
    "axios": "^1.6.2",
    "expo": "^54.0.30",
    "expo-camera": "~17.0.10",
    "expo-file-system": "~19.0.21",
    "expo-image-picker": "~17.0.10",
    "expo-location": "^19.0.8",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~15.0.8",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "i18next": "^23.7.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^14.0.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-signature-canvas": "^3.0.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "^0.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~18.2.45",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.16",
    "typescript": "^5.1.3"
  },
  "private": true
}
