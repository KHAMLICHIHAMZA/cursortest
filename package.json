{
  "name": "malocauto",
  "version": "2.0.0",
  "private": true,
  "description": "MalocAuto — Application unifiée sur un seul port (8080)",
  "scripts": {
    "dev": "concurrently -n backend,web,proxy -c blue,green,cyan \"npm run dev:backend\" \"npm run dev:web\" \"npm run dev:proxy\"",
    "dev:proxy": "cd proxy && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:web": "cd frontend-web && npm run dev -- -p 3001",
    "postinstall": "npm run install:proxy",
    "install:proxy": "cd proxy && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.21",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.575.0",
    "next": "^16.1.6",
    "puppeteer": "^24.37.2",
    "react": "^19.2.4",
    "tailwindcss": "^4.2.0"
  }
}
